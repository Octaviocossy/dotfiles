{
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":sp"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": [":vsp"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["eslint.executeAutofix"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "d"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.files.action.focusFilesExplorer"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.scm.focus"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "terminal.integrated.fontFamily": "Dank Mono, FiraCode Nerd Font Mono",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "reactSnippets.settings.importReactOnTop": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "Dank Mono, FiraCode Nerd Font Mono",
  "git.openRepositoryInParentFolders": "always",
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.8,
  "editor.fontWeight": "normal",
  "editor.fontSize": 13,
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.sideBar.location": "right",
  "explorer.confirmDragAndDrop": false,
  "window.commandCenter": false,
  "workbench.colorTheme": "Bearded Theme Arc",
  "workbench.iconTheme": "bearded-icons",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  }
}