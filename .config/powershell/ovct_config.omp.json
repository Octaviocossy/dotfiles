{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#16C60C",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#61D6D6",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#16C60C",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#16C60C>îœ¥ </>{{ .HEAD }}<#16C60C></><#881798> ><#881798>",
          "type": "git"
        },
        {
          "foreground": "#C50F1F",
          "style": "plain",
          "template": " \u2717",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}